    public static final RegistryObject<Block> BLOB = registerBlock("blob",
            () -> new BlobBlock(BlockBehaviour.Properties.copy(Blocks.IRON_BLOCK).noOcclusion().strength(6f).requiresCorrectToolForDrops()));
